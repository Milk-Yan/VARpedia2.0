<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<GridPane id="red" prefHeight="600.0" prefWidth="1024.0" stylesheets="@/main/resources/css/ListStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controllers.createCreation.AudioSearch">
   <columnConstraints>
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" percentWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" percentHeight="12.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" percentHeight="56.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" percentHeight="15.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" percentHeight="15.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Which wikit term would you like to make a creation of?" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
            <font>
                <Font size="26.0" />
            </font>
        </Text>
        <ListView fx:id="_wikitTerm" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
         <GridPane.margin>
            <Insets left="20.0" right="20.0" />
         </GridPane.margin>
      </ListView>
        <Button fx:id="_enterBtn" mnemonicParsing="false" onAction="#enter" prefHeight="42.0" prefWidth="600.0" text="Enter" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
            <font>
                <Font size="26.0" />
            </font>
        </Button>
        <Button fx:id="_mainMenuBtn" mnemonicParsing="false" onAction="#mainMenu" prefHeight="42.0" prefWidth="600.0" text="Quit to Main Menu" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER">
            <font>
                <Font size="26.0" />
            </font>
        </Button>
   </children>
</GridPane>
