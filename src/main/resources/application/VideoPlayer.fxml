<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controllers.VideoPlayerController">
   <children>
      <MediaView fx:id="_viewer" fitHeight="300.0" fitWidth="500.0">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </MediaView>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Button fx:id="_playPauseBtn" mnemonicParsing="false" onAction="#playPause" text="Pause" />
            <Button fx:id="_fastForwardBtn" mnemonicParsing="false" onAction="#fastForward" text="Fast Forward" />
            <Button fx:id="_slowDownBtn" mnemonicParsing="false" onAction="#slowDown" text="Slow Down" />
            <Button fx:id="_forwardBtn" mnemonicParsing="false" onAction="#forward" text="Forward" />
            <Button fx:id="_backwardBtn" mnemonicParsing="false" onAction="#backward" text="Backward" />
            <Button fx:id="_muteBtn" mnemonicParsing="false" onAction="#mute" text="Mute" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</VBox>
