<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="_container" alignment="CENTER"
      maxHeight="-Infinity"
      maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0"
      spacing="30.0" xmlns="http://javafx.com/javafx/8.0.141"
      fx:controller="main.java.controllers.createCreation.AudioSearch">
    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0"
              text="Which wikit term would you like to make a creation of?">
            <font>
                <Font size="19.0"/>
            </font>
        </Text>
        <ListView fx:id="_wikitTerm" prefHeight="200.0" prefWidth="200.0"/>
        <Button fx:id="_enterBtn" mnemonicParsing="false" onAction="#enter" prefWidth="300.0"
                text="Enter">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
        <Button fx:id="_mainMenuBtn" mnemonicParsing="false" onAction="#mainMenu" prefWidth="300.0"
                text="Quit to Main Menu">
            <font>
                <Font size="18.0"/>
            </font>
        </Button>
    </children>
    <padding>
        <Insets left="10.0" right="10.0"/>
    </padding>
</VBox>
